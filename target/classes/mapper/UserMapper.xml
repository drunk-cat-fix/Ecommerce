<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.murray.e_commercesystem.mapper.UserMapper">

    <select id="selectAllUsers" resultType="com.murray.e_commercesystem.entities.User">
        SELECT id,username,password from t_test
    </select>
    <select id="paging" resultType="com.murray.e_commercesystem.entities.User">
        select * from t_test where username = #{name}
    </select>
</mapper>